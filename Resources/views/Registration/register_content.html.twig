{% trans_default_domain 'FOSUserBundle' %}

<div class="row">
    <div class="col-md-6">
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST">
            {% form_theme form 'MsiAdminBundle:Form:form.html.twig' %}
            {{ form_rest(form) }}
            <input class="btn btn-default" type="submit" value="{{ 'registration.submit'|trans }}">
        </form>
    </div>
</div>
